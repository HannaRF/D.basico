from random import randrange
class Pessoa:
    def __init__(self,sexo,cor_raca,regiao,renda_percapita,renda_media,renda_mediana,rede_ensino,modalidade,turno,dif_financeira,dif_acesso_local,dif_cumprir_horario,falta_tempo,outra_dif,sem_dif):
        self.sexo = sexo
        self.cor_raca = cor_raca
        self.regiao = regiao
        self.renda_percapita = renda_percapita
        self.renda_media = renda_media
        self.renda_mediana = renda_mediana
        self.rede_ensino = rede_ensino
        self.modalidade = modalidade
        self.turno = turno
        self.dif_financeira = dif_financeira
        self.dif_acesso_local = dif_acesso_local
        self.dif_cumprir_horário = dif_cumprir_horario
        self.falta_tempo = falta_tempo
        self.outra_dif = outra_dif
        self.sem_dif = sem_dif
 
a1=['F','M']
a2=['branco','preto_pardo']
a3=['N','S','SE','NE','CO']
a7=['pública','particular']
a8=['presencial','à distância']
a9=['manhã','tarde','noite','parcial']#atenção,relacionada
a=[False,True]

populaçao = [Pessoa(a1[randrange(self,0,2,1,int)], a2[randrange(self,0,2,1,int)],a3[randrange(self,0,5,1,int),randrange...]) for i in range(100)]

return(populaçao[0].peso, populaçao[0].idade)
